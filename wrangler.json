{
  "name": "jaukuma-astro",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "vars": {
    "HOSTNAME": "www.jaukuma.lt"
  },
  "routes": [
     "www.jaukuma.lt/*", 
    { "pattern": "www.jaukuma.lt", "custom_domain": true },
    { "pattern": "jaukuma.lt", "custom_domain": true }
  ],
  "env": {
    "development": {
      "vars": {
        "HOSTNAME": "dev.jaukuma.lt"
      },
      "routes": [
        "dev.jaukuma.lt/*", 
        { "pattern": "dev.jaukuma.lt", "custom_domain": true }
      ]
    }
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}