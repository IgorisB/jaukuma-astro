{
  "name": "jaukuma-astro",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "vars": {
    "PROD_SITE": "www.jaukuma.lt"
  },
  "routes": [
    { "pattern": "jaukuma.lt/*", "zone_id": "d9ea705d63e6bfcf871820808c860a69" },
    { "pattern": "www.jaukuma.lt/*", "zone_id": "d9ea705d63e6bfcf871820808c860a69" }
  ],
  "env": {
    "development": {
      "vars": {
        "PROD_SITE": "dev.jaukuma.lt"
      },
      "routes": [
        { "pattern": "dev.jaukuma.lt/*", "zone_id": "d9ea705d63e6bfcf871820808c860a69" }
      ]
    }
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}