{
  "name": "jaukuma-website",
  "pages_build_output_dir": "./dist",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "HOSTNAME": "www.jaukuma.lt",
    "ROBOTS": "Allow"
  },
  "env": {
    "preview": {
      "vars": {
        "HOSTNAME": "dev.jaukuma.lt",
        "ROBOTS": "Disallow"
      }
    }
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}