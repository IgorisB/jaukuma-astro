---
import Layout from '../../layouts/Layout.astro';
import PageIndexBody from '../../components/pages/page.index.astro';
import { useTranslationsFromUrl, getLangFromUrl, getStaticPaths } from '../../lib/utils';
const lang = getLangFromUrl(Astro.url);
const t = useTranslationsFromUrl(Astro.url);
export { getStaticPaths };
---
<Layout title={t('page.index.title')} locale={lang}>
  <PageIndexBody locale={lang} />
</Layout>
